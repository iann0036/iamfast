{
  "StartAWRONG": "Parallel",
  "States": {
    "Parallel": {  
      "Type": "Parallel",
      "Branches": [
        {
          "StartAWRONG": "Assign",
          "States": {
            "Assign": {
              "Type": "Task",
              "ResultPath": "$.vars.return_var",
              "Resource": "[!lambda[worker]arn]",
              "Retry": [
                {
                  "ErrorEquals": [
                    "Lambda.ServiceException",
                    "Lambda.AWSLambdaException",
                    "Lambda.SdkClientException"
                  ],
                  "IntervalSeconds": 2,
                  "MaxAttempts": 6,
                  "BackoffRate": 2
                }
              ],
              "Next": "Return",
              "End": true
            }
          }
        }
      ],
      "ResultPath": "$.result",
      "Next": "Return_2"
    },
    "Return_2": {
      "Type": "Pass",
      "InputPath": "$.result",
      "End": true
    }
  }
}